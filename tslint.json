{
  "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "no-console": false,
    "no-boolean-literal-compare": true,
    "variable-name": false
  }
}
